/**index.wxss**/

page {
  width: 100%;
  height: 100%;
}


.head{
  position: absolute;
  width: 100%;
  height: 56px;
  left: 0px;
  top: 48rpx;
  display: block;
  line-height: 56px;
  text-align: center;
  font-size: 16px;
  font-style: normal;
  font-weight: 500;
  color: #76839c;
}

.container{
  width: 100%;
  height: 100%;
  background: #F5F7FA;
}

.search-box{
  width: 96%;
  border-radius: 80rpx;
  background: #EBEEF5;
  color: #76839c;
  line-height: 39px;
}

.search-text{
  position: absolute;
  line-height: 35px;
  color: #76839c;
  font-weight: 500;
  font-style: normal;

}

.cont{
  position: absolute;
  margin-block: 4px;
  left: -50px;
  width: 400px;
  height: 100px; 
  box-sizing: border-box;
  border-radius: 12px;
  background-color: #dee3eb;
  overflow: hidden;
  
}